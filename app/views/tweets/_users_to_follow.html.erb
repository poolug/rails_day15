<% current_user.you_follow.each do |user| %>
    <% if user.id != current_user.id %>
        <%= image_tag user.photo, class:"rounded-circle", style:"border: 2px solid black" %>
        <%= link_to follow_path(user.id), class:"btn btn-primary", method: :post do %>
            <%= "Seguir" %>
        <% end %>
    <% end %>
<% end %>
